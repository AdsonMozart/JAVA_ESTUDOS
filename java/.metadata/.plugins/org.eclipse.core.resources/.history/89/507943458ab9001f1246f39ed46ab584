package streams;

import java.util.function.Function;
import java.util.function.UnaryOperator;

public class DesafioUtilitarios {

	
	
	public final static Function<Integer, String> binarizando = exemplo -> Integer.toBinaryString(exemplo);
	public final static UnaryOperator<String> invertendo = exemplo -> exemplo.reverte
//	public final static Function<Integer, String> binarizanssdo = exemplo -> Integer.toBinaryString(exemplo);
	
	public String reverter() {
		StringBuilder sb;
		return sb.reverse().toString();
	}
}
